{
  "tags": {
      "allowUnknownTags": false
    },
  "source": {
      "include": "./source/",
      "includePattern": ".js$",
      "excludePattern": "(node_modules/|docs/|test.js)"
    },
  "plugins": [
      "plugins/markdown"
    ],
  "opts": {
      "template": "./node_modules/docdash_im_fork",
      "encoding": "utf8",
      "destination": "docs/",
      "recurse": true,
      "verbose": true
    },
  "templates": {
      "cleverLinks": false,
      "monospaceLinks": false,
      "default": { "outputSourceFiles": false }
   },
  "docdash": {
      "static": true,
      "sort": true
    }
}
