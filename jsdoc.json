{
  "tags": {
    "allowUnknownTags": false
  },
  "source": {
    "include": "./",
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs/|test.js/|docdash_fork)"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "opts": {
    "template": "./docdash_fork",
    "encoding": "utf8",
    "destination": "docs/",
    "recurse": true,
    "verbose": true
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "default": { "outputSourceFiles": false }
  },
  "docdash": {
    "static": true,
    "sort": true
  }
}
